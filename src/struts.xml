<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">

<!-- Strutsの詳細設定 -->
<struts>
	<constant name="struts.devMode" value="true"/>
		<!-- 対象のpackageを指定 -->
		<package name="com.internousdev.personal.action" extends="struts-default">

			<action name="StartAction" class="com.internousdev.personal.action.StartAction" method="execute">
				<result name="login">login.jsp</result>
				<result name="success">buyItem.jsp</result>
			</action>

			<action name="InquiryAction" class="com.internousdev.personal.action.InquiryAction" method="execute">
				<result name="success">inquiry.jsp</result>
			</action>

			<action name="InquiryCompleteAction" class="com.internousdev.personal.action.InquiryCompleteAction" method="execute">
				<result name="success">inquiryComplete.jsp</result>
			</action>

			<action name="GoHomeAction" class="com.internousdev.personal.action.GoHomeAction" method="execute">
				<result name="success">home.jsp</result>
			</action>

			<action name="LoginAction" class="com.internousdev.personal.action.LoginAction" method="execute">
				<result name="success">buyItem.jsp</result>
				<result name="error">login.jsp</result>
			</action>

			<action name="ItemCreateAction" class="com.internousdev.personal.action.ItemCreateAction" method="execute">
				<result name="success">itemCreate.jsp</result>
			</action>

			<action name="ItemCreateConfirmAction" class="com.internousdev.personal.action.ItemCreateConfirmAction" method="execute">
				<result name="success">itemCreateConfirm.jsp</result>
				<result name="error">itemCreate.jsp</result>
			</action>

			<action name="ItemCreateCompleteAction" class="com.internousdev.personal.action.ItemCreateConfirmAction" method="execute">
				<result name="success">itemCreateComplete.jsp</result>
			</action>

			<action name="UserCreateAction" class="com.internousdev.personal.action.UserCreateAction" method="execute">
				<result name="success">userCreate.jsp</result>
			</action>

			<action name="UserCreateConfirmAction" class="com.internousdev.personal.action.UserCreateConfirmAction" method="execute">
				<result name="success">userCreateConfirm.jsp</result>
				<result name="error">userCreate.jsp</result>
			</action>

			<action name="UserCreateCompleteAction" class="com.internousdev.personal.action.UserCreateCompleteAction" method="execute">
				<result name="success">userCreateComplete.jsp</result>
			</action>

			<action name="BuyItemAction" class="com.internousdev.personal.action.BuyItemAction" method="execute">
				<result name="success">buyItemConfirm.jsp</result>
				<result name="error">buyItem.jsp</result>
			</action>

			<action name="BuyItemFinishAction" class="com.internousdev.personal.action.BuyItemFinishAction" method="execute">
				<result name="success">buyItemComplete.jsp</result>
			</action>

			<action name="MyPageAction" class="com.internousdev.personal.action.MyPageAction" method="execute">
				<result name="success">myPage.jsp</result>
				<result name="error">error.jsp</result>
			</action>

			<action name="LogoutAction" class="com.internousdev.personal.action.LogoutAction" method="execute">
				<result name="success">home.jsp</result>
			</action>













		</package>
</struts>